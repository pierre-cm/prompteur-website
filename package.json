{
  "name": "prompteur-website",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "fetch": "bun run scripts/fetch.ts",
    "build": "bun run fetch && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.70.0",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "highlight.js": "^11.9.0",
    "marked": "^11.1.1",
    "prompteur": "github:pierre-cm/prompteur"
  },
  "trustedDependencies": ["prompteur"]
}
